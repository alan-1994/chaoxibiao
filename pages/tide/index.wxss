ec-canvas {
    height: 500rpx;
    width: 100%;
}

.tidedesc {
    font-size: 24rpx;
    text-align: center;
}

.previous {
    float: left;
    margin-left: 20rpx;
}

.next,.previous {
    background-color: #1f5b59;
    color: #fff;
    font-size: 28rpx;
    padding: 12rpx 35rpx;
}

.next {
    float: right;
    margin-right: 20rpx;
}

.btn {
    background-color: #1f5b59;
    color: #fff;
    font-size: 28rpx;
    margin: 20rpx;
}

.btn-hover {
    background-color: #3ba7a3;
}

.title1 {
    font-size: 30rpx;
    margin: 5rpx 0;
}

.title2 {
    color: #bbb;
    font-size: 25rpx;
}

.edit {
    background-color: #1f5b59;
    color: #fff;
    font-size: 20rpx;
    padding: 6rpx 20rpx;
    position: absolute;
    right: 5rpx;
    top: 9rpx;
}

.picker {
    color: #ff6a00;
    font-size: 32rpx;
    height: 90rpx;
    margin: 15rpx 10rpx;
    text-align: center;
    text-decoration: underline;
}

.picker>view {
    display: inline-block;
    margin: auto;
    padding: 27rpx;
    width: 70rpx;
}

.picker>view:first-child {
    width: 200rpx;
}

.nearby {
    display: inline-block;
    font-size: 30rpx;
    margin-left: 15rpx;
}

.nearby-name {
    color: #9b9b9b;
    display: inline-block;
    font-size: 28rpx;
    margin: 0 15rpx;
}

.nearby-gl {
    font-size: 20rpx;
}

.nearby-container {
    margin-top: 20rpx;
}

.today {
    margin: 5rpx 0;
    padding: 6rpx 0;
    position: relative;
    text-align: center;
}

.today .left {
    display: inline-block;
    text-align: right;
    vertical-align: top;
    width: 220rpx;
}

.today .left image {
    background: #fff;
    border-radius: 110rpx;
    height: 160rpx;
    padding: 10rpx;
    width: 160rpx;
}

.today .right {
    display: inline-block;
    width: calc(100% - 220rpx);
}

.today .ghdesc {
    color: #ff6a00;
    font-size: 23rpx;
    margin: 8rpx 0;
}

.today .title {
    height: 35rpx;
    line-height: 100%;
    width: "100%";
}

.today .title .column {
    display: inline-block;
    font-size: 28rpx;
    width: 90rpx;
}

.today .title .column1 {
    width: 200rpx;
}

.today .title .column2 {
    width: 49%;
}

.today .value {
    height: 32rpx;
    line-height: 100%;
    width: "100%";
}

.today .value .column {
    color: #9b9b9b;
    display: inline-block;
    font-size: 26rpx;
    width: 90rpx;
}

.today .value .column1 {
    width: 200rpx;
}

.today .value .column2 {
    width: 49%;
}

.today .value2 {
    height: auto;
    line-height: 100%;
    width: "100%";
}

.today .value2 .column {
    color: #9b9b9b;
    display: inline-block;
    font-size: 26rpx;
    width: 110rpx;
}

.today .title2 {
    background-color: #1f5b59;
    padding: 10rpx 0;
}

.today .title2 .column {
    color: #fff;
    font-size: 26rpx;
}

.today .more text {
    color: #ff6a00;
    font-size: 30rpx;
    margin: 10rpx 25rpx 0;
    padding: 8rpx;
    text-decoration: underline;
}

.today .day15,.today .hour24 {
    display: none;
    height: auto;
    left: 0;
    overflow: auto;
    padding: 20rpx 0 40rpx;
    top: 570rpx;
    width: 100%;
    z-index: 999;
}

.today .show {
    display: block;
}

.today .lable {
    color: #fff;
    font-size: 32rpx;
    margin: 5rpx;
}

.today .date {
    font-size: 25rpx;
    margin: 5rpx 0 18rpx;
}

.today .close {
    color: #ff6a00;
    font-size: 30rpx;
    position: absolute;
    right: 20rpx;
    text-decoration: underline;
    top: 40rpx;
    z-index: 1000;
}

.see {
    border-top: 1rpx dotted #eee;
    margin: 15rpx 0;
    padding: 6rpx 20rpx;
    text-align: right;
}

.see .column {
    display: inline-block;
}

.see .column image {
    height: 26rpx;
    margin-left: 20rpx;
    width: 26rpx;
}

.see .column text {
    color: grey;
    font-size: 24rpx;
    margin-left: 10rpx;
}

.desc {
    color: grey;
    margin-top: 20rpx;
    text-shadow: 2rpx 2rpx 2rpx 5rpx #555;
}

.desc,.tides_desc {
    font-size: 20rpx;
    text-align: center;
}

.tides_desc {
    color: #fff;
}

.tides_desc label {
    border-radius: 100rpx;
    font-size: 18rpx;
    margin: 0 10rpx;
    padding: 0 13rpx;
}

.weather {
    font-size: 24rpx;
}

.weather .title {
    background-color: #1f5b59;
    font-size: 28rpx;
    padding: 8rpx 10rpx;
}

.weather .title .temperature {
    float: right;
}

.weather .title .ghdesc {
    color: #ff6a00;
    text-decoration: underline;
}

.weather .row {
    color: #9b9b9b;
}

.weather .row .column {
    display: inline-block;
    padding: 5rpx 10rpx;
    vertical-align: top;
    width: 77rpx;
}

.weather .row .column2 {
    width: 258rpx;
}

.weather .row .column3 {
    width: 320rpx;
}

.weather .row .right {
    color: #fff;
    text-align: right;
}

.add {
    background-color: #1f5b59;
    border-radius: 10rpx;
    font-size: 16rpx;
    left: 30rpx;
    padding: 5rpx 12rpx 8rpx;
    position: absolute;
    text-align: center;
    top: 45rpx;
}

.add image {
    height: 55rpx;
    width: 55rpx;
}

@-webkit-keyframes bgposition {
    0% {
        background-position: 0 0;
    }

    100% {
        background-position: 100% 0;
    }
}

@keyframes bgposition {
    0% {
        background-position: 0 0;
    }

    100% {
        background-position: 100% 0;
    }
}

.zhuanhuan {
    animation: bgposition 5s linear infinite alternate;
    background: linear-gradient(40deg,#1f5b59 20%,#e91e1e);
    background-position: 0 0;
    background-size: 200% 100%;
    border-radius: 10rpx;
    left: auto;
    right: 30rpx;
}

.data_type {
    background-color: #1f5b59;
    border-bottom: 3rpx solid #1f5b59;
    height: 41rpx;
    overflow: hidden;
    text-align: center;
}

.data_type .but {
    border-left: 1rpx solid #122b3a;
    border-radius: 0;
    border-right: 1rpx solid #122b3a;
    color: #fff;
    display: inline-block;
    font-size: 22rpx;
    margin-top: -3rpx;
    padding: 10rpx;
    vertical-align: top;
    width: 150rpx;
}

.data_type .select {
    background-color: #f36;
}

.data_type .but:first-of-type {
    border-right: 0;
}

.data_type .but:last-child {
    border-left: 0;
}

.GG100 {
    bottom: 340rpx;
    height: 85rpx;
    left: 5rpx;
    position: absolute;
    width: 85rpx;
}